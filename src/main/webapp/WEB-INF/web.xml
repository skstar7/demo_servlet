<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>

		<servlet-name>a</servlet-name>

		<servlet-class>Sample.Basics</servlet-class>

	</servlet>


	<servlet>

		<servlet-name>b</servlet-name>

		<servlet-class>Sample.Addition</servlet-class>

	</servlet>
	
	<servlet>

		<servlet-name>water</servlet-name>

		<servlet-class>calculator.Servlet_calc</servlet-class>

	</servlet>




	<servlet-mapping>
		<servlet-name>a</servlet-name>

		<url-pattern>/xyz</url-pattern>

	</servlet-mapping>



	<servlet-mapping>
		<servlet-name>b</servlet-name>

		<url-pattern>/add</url-pattern>

	</servlet-mapping>
	



</web-app>
